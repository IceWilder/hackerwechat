    <script>
      bootbox.setLocale("zh_CN");
    $(function () {
      var jscript = '<%= jscript%>';
      console.log("加载的脚本",jscript)
      if(jscript){
        $.getScript('<%= jscript%>');
      }

      var activeMenu = '<%=active%>';
      if(activeMenu){
        $(".<%=active%>").addClass("active");
        $(".<%=active%>").parent().parent().addClass("active");
      }

      var username = getCookie("username");
      if(username){
        $(".username").text(username);
      }
      var createdDate = getCookie("create_time");
      if(createdDate){
        $(".createdDate").text(createdDate.substring(0,10));
      }

      var portrait = getCookie("portrait");
      if(portrait){
        $(".userImg").attr("src",portrait);
      }

      $(".head-img").click(function(){
        location.href = "/pam/profile";
      });

      $("#modPwdBtn").click(function(){
        bootbox.dialog({
                  title: "修改密码",
                  size:'small',
                  message: '<div class="row" >  ' +
                  '<div class="col-md-10" style="padding-left:18%;"> ' +
                  '<form class="form-horizontal"> ' +
                  '<div class="form-group"> ' +
                  '<label class="control-label" for="oldpwd">旧密码</label> ' +
                  '<input id="oldpwd" name="oldpwd" type="password" placeholder="旧密码" class="form-control"> ' +
                  '</div> ' +
                  '<div class="form-group"> ' +
                  '<label class="control-label" for="newpwd">新密码</label> ' +
                  '<input id="newpwd" name="newpwd" type="password" placeholder="新密码" class="form-control"> ' +
                  '</div> ' +
                  '<div class="form-group"> ' +
                  '<label class="control-label" for="renewpwd">重复新密码</label> ' +
                  '<input id="renewpwd" name="renewpwd" type="password" placeholder="重复新密码" class="form-control"> ' +
                  '</div> ' +
                  '</form> </div>  </div>',
                  buttons: {
                    success: {
                      label: "保存",
                      className: "btn-primary",
                      callback: function (result) {
                         var oldpwd = $("#oldpwd").val();
                         var newpwd = $("#newpwd").val();
                         var renewpwd = $("#renewpwd").val();

                        if(!oldpwd || !newpwd || !renewpwd){
                          bootbox.alert("密码不能为空");
                          return;
                        }

                        if(newpwd != renewpwd){
                          bootbox.alert("重复新密码不匹配！");
                          return;
                        }

                        $.ajax({
                          url:"/doEditPwd",
                          type:"post",
                          data: {password:newpwd,oldpassword:oldpwd} ,
                          dataType:'json',
                          success: function(d){
                            bootbox.alert(d.msg);
                          },
                          error: function(x, t, e) {}
                        });//提交完成



                      }
                    }
                  }
                }
        );
      });

      $('input').iCheck({
        checkboxClass: 'icheckbox_square-blue',
        radioClass: 'iradio_square-blue',
        increaseArea: '20%' // optional
      });

    });
    </script>
