<style type="text/css">
    .content-header{
        margin-bottom: 10px;
        height: 70px;
        line-height: 70px;
    }
    .btn{
        position: relative;
        bottom: 7px;
    }
    p{
        display: block;
        min-height:40px;
        line-height: 40px;
    }
    .bill{
        width: 430px;
        height: 224px;
        margin-top: 20px;
        background-color: darkgrey;

    }
    .bill img{
        width: 100%;
        height: 100%;
    }
    .user-head{
        float: left;
        position:relative;
        left: 10px;
        width: 100px;
        height: 100px;
    }
    .head-wrapper{
        height: 170px;
    }
    .panel-body{
        height: 200px;
    }
    .panel{
        min-height: 200px;
    }
</style>
<section class="content-header">
    <h1 style="display:inline-block;">赛事名称:<%=data.compete_name%></h1>&nbsp;&nbsp;

</section>

<section class="content">
    <%if(data.status==0){%>
    <p>赛事状态:赛后展示</p>
    <%}else if(data.status==1){%>
    <p>赛事状态:报名中</p>
    <%}else{%>
    <p>赛事状态:报名结束</p>
    <%}%>
    <p>主办方:<%=data.host%></p>
    <p>赛事地点:<%=data.address%></p>
    <p>赛事时间:<%=data.time%></p>
    <%if(data.online==0){%>
    <p>赛事线上报名:不允许线上报名</p>
    <%}else{%>
    <p>赛事线上报名:允许线上报名</p>
    <%}%>
    <p>赛事官网链接:<%=data.compete_url%></p>
    <button class="btn btn-primary btn-lg active" id="btn-come">点击量:<%=data.click_num%></button>
    <button class="btn btn-primary btn-lg active" id="btn-change" >修改比赛</button>
</section>
<section class="content">
    赛事海报:
    <div class="bill">
        <img src="<%=data.first_bill%>">
    </div>
    <div class="bill">
        <img src="<%=data.second_bill%>">
    </div>
    <div class="bill">
        <img src="<%=data.three_bill%>">
    </div>
</section>
<div style="border-bottom:1px solid darkgrey;"></div>
<section class="content">
    <%if(data.video_nums==0){%>
    暂无赛事视频
    <%}else{%>
    赛事视频:
    <ul class="nav nav-tabs nav-justified">
    </ul>
    <%}%>
    <div class="panel panel-default">
    </div>

</section>

<!--<script src="jquery.js"></script>-->
<script type="text/javascript" src="/plugins/My97DatePicker/WdatePicker.js"></script>

<script>
    var compete_id=<%=data.compete_id%>
</script>