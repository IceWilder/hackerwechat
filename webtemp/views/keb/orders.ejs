<style>
    .top{
        margin-top: 10px;
    }
    .input_wenben{
        height: 32px;
    }
    .select_wenben{
        height: 33px;
    }
</style>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            订单列表
            <small>所有历史订单</small>
        </h1>
        <ol class="breadcrumb">
            <li class="active"><i class="fa fa-list-alt"></i> 订单管理</li>
            <li class="active">订单列表</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">

        <div class="row" style="margin-top: 20px;">
                <div class="col-xs-12">

                    <div class="box">
                        <div class="box-body">
                            <div style="padding: 20px 0;">
                                <div class="col-sm-2 col-xs-4">
                                    <label style="float: left;margin-top: 5px;">订单号：</label>
                                    <input type="text" id="keyword" class="form-control" class="input_wenben" style="width:70%;" placeholder="拍品名称"/>
                                </div>

                                <div class="col-sm-2 col-xs-4">
                                    <label style="float: left;margin-top: 5px;">起始时间：</label>
                                    <span class="input-group date form_date col-md-8">
                                          <input class="form-control" type="text" value="" readonly id="startDate">
                                          <span class="input-group-addon">
                                          <span class="glyphicon glyphicon-calendar"></span></span>
                                    </span>
                                </div>
                                <div class="col-sm-2 col-xs-4">
                                    <label style="float: left;margin-top: 5px;">结束时间：</label>
                                    <span class="input-group date form_date col-md-8">
                                          <input class="form-control" type="text" value="" readonly id="endDate">
                                          <span class="input-group-addon">
                                          <span class="glyphicon glyphicon-calendar"></span></span>
                                    </span>
                                </div>

                                <div class="col-sm-2 col-xs-4">
                                    <label style="float:left;margin-top:5px;">订单状态：</label>
                                    <select class="form-control" style="width:70%" id="prod_status">
                                        <option value="">全部</option>
                                        <option value="1">等待买家付款</option>
                                        <option value="2">等待发货</option>
                                        <option value="3">已发货</option>
                                        <option value="4">退款中</option>
                                        <option value="5">关闭的订单</option>
                                        <option value="6">成功的订单</option>
                                    </select>
                                </div>
                                <button type="submit" class="btn btn-primary" id="searchBtn">搜&nbsp;索</button>
                            </div>


                            <table id="orders" class="table table-bordered table-striped">
                                <thead>
                                <tr>
                                    <th>竞标者</th>
                                    <th>地址</th>
                                    <th>手机号</th>
                                    <th>竞拍品</th>
                                    <th>支付金额</th>
                                    <th>支付状态</th>
                                    <th>订单状态</th>
                                    <th>操作</th>
                                </tr>
                            </table>
                        </div><!-- /.box-body -->
                    </div><!-- /.box -->

                </div><!-- /.col -->
            </div><!-- /.row -->

    </section><!-- /.content -->
</div><!-- /.content-wrapper -->